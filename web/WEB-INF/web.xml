<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    <servlet>
        <servlet-name>ClientesServlet</servlet-name>
        <servlet-class>cr.ac.una.proyecto.navegacion.controller.ClientesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ClientesServlet</servlet-name>
        <url-pattern>/ClientesServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>VehiculosServlet</servlet-name>
        <servlet-class>cr.ac.una.proyecto.navegacion.controller.VehiculosServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VehiculosServlet</servlet-name>
        <url-pattern>/VehiculosServlet</url-pattern>
    </servlet-mapping>
     <servlet>
        <servlet-name>UsuariosServlet</servlet-name>
        <servlet-class>cr.ac.una.proyecto.navegacion.controller.UsuariosServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UsuariosServlet</servlet-name>
        <url-pattern>/UsuariosServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>cr.ac.una.proyecto.navegacion.controller.Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
     <servlet>
        <servlet-name>ChoferesServlet</servlet-name>
        <servlet-class>cr.ac.una.proyecto.navegacion.controller.ChoferesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ChoferesServlet</servlet-name>
        <url-pattern>/ChoferesServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
